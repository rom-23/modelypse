            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title reveal-3">
                        <span>Modèles à base de maquettes</span>
                        <h2></h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="feature-carousel owl-carousel reveal-4">
                    {% for model in models %}
                        <div class="col-lg-12">
                            <a href="{{ path('model_show', {id:model.id, slug:model.slug}) }}">
                                <div class="feature-item">
                                    <div class="fi-pic set-bg" data-setbg="{{ asset('uploads/images/models/'~ model.filename) }}">
                                        <div class="pic-tag">
                                            <div class="f-text">feature</div>
                                            <div class="s-text">For Sale</div>
                                        </div>
                                        <div class="feature-author">
                                            <div class="fa-pic">
                                                {% if model.filename %}
                                                    <img src="{{ vich_uploader_asset(model, 'imageFile') }}" alt="card-img-top">
                                                {% else %}
                                                    <img src="{{ '/uploads/images/models/empty.png' }}" alt="card-img-top">
                                                {% endif %}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="fi-text">
                                        <div class="inside-text">
                                            <h4>{{ model.name }}</h4>
                                            <ul>
                                                <li><i class="fa fa-map-marker"></i>{{ model.description|raw }}</li>
                                                <li><i class="fa fa-tag"></i>{{ model.price }} e</li>
                                            </ul>
