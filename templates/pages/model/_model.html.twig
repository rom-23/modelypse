<div class="card mb-4">
    {% if model.filename %}
        <img src="{{ vich_uploader_asset(model, 'imageFile') }}" alt="card-img-top">
    {% else %}
        <img src="{{ '/images/models/empty.jpg' }}" alt="card-img-top">
    {% endif %}
    <div class="card-body">
        <h5 class="card-title">{{ model.name }}</h5>
        <p class="card-text">{{ model.description|raw }})<br>
        <span>{{ model.price }} e</span>
        <a href="{{ path('model_show', {id:model.id, slug:model.slug}) }}" class="btn btn-success pull-right">Read</a>
     </div>
</div>
